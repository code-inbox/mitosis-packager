{
  "name": "@magicbell/buttons",
  "version": "1.3.0",
  "description": "MagicBell Buttons SDK",
  "author": "MagicBell <bot@magicbell.io> (https://magicbell.com)",
  "contributors": [
    "Stephan Meijer <stephan.meijer@gmail.com>",
    "Matthew Oxley <matt@magicbell.io>"
  ],
  "license": "SEE LICENSE IN LICENSE",
  "sideEffects": false,
  "files": [
    "/dist",
    "/src",
    "LICENSE"
  ],
  "homepage": "https://magicbell.com",
  "keywords": [
    "magicbell",
    "notifications",
    "notification center",
    "notification inbox",
    "api",
    "PWA"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:magicbell-io/buttons.git"
  },
  "bugs": {
    "url": "https://github.com/magicbell-io/buttons/issues"
  },
  "scripts": {
    "mitosis": "mitosis build",
    "clean": "rimraf ./packages/react/dist ./packages/svelte/dist ./packages/vue/dist ./packages/solid/dist",
    "bundle:vue": "vite build --config packages/vue/vite.bundle.mts",
    "bundle:react": "vite build --config packages/react/vite.bundle.mts",
    "bundle:svelte": "vite build --config packages/svelte/vite.bundle.mts",
    "bundle:solid": "vite build --config packages/solid/vite.bundle.mts",
    "bundle:all": "yarn run clean && yarn run bundle:vue & yarn run bundle:react & yarn run bundle:svelte & yarn run bundle:solid && yarn test",
    "build": "yarn run mitosis && yarn run bundle",
    "test:react": "yarn cypress open --project ./packages/react --component",
    "test:svelte": "yarn cypress open --project ./packages/svelte --component",
    "test:vue": "yarn cypress open --project ./packages/vue --component",
    "test:solid": "yarn cypress open --project ./packages/solid --component",
    "test": "node cypress/test-all.mjs"
  },
  "devDependencies": {
    "@builder.io/eslint-plugin-mitosis": "^0.0.14",
    "@cypress/mount-utils": "^4.0.0",
    "@dream2023/cypress-ct-solid-js": "^1.0.0",
    "@sveltejs/vite-plugin-svelte": "^2.4.6",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.6.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/rollup-plugin-peer-deps-external": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@vitejs/plugin-react": "^4.0.4",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "chalk": "4.1.2",
    "cypress": "^13.2.0",
    "eslint": "^8.49.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.1",
    "rollup": "^3.29.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "solid-js": "^1.7.11",
    "solid-styled-components": "^0.28.5",
    "svelte-preprocess": "^5.0.4",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-css-injected-by-js": "^3.3.0",
    "vite-plugin-dts": "^3.5.3",
    "vite-plugin-solid": "^2.7.0",
    "vue": "^3.3.4"
  },
  "dependencies": {
    "@builder.io/mitosis": "^0.0.119",
    "@builder.io/mitosis-cli": "^0.0.77"
  },
  "peerDependencies": {}
}
